aside 
	position fixed
	left 0
	top 0
	bottom 0
	width asideSize
	z-index 2
	border-right solid 1px #444
	transition all 0.5s ease-in-out
	transform-origin left center 0
	section
		box-shadow:0 0 15px #666
		li
			padding 25px
			list-style none
			border-bottom rowBorder
			background linear-gradient(top, #fff, rgba(256, 256, 256, 0.2) 100)
			font-size: 10pt
			p
				display inline-block
				padding 0
				margin 0
			i
				margin-right 5px


